{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'Clients',\n  data() {\n    return {\n      clients: []\n    };\n  },\n  mounted() {\n    this.getClients();\n  },\n  methods: {\n    async getClients() {\n      this.$store.commit('setIsLoading', true);\n      await axios.get('/api/v1/clients/').then(response => {\n        // console.log(response.data);\n        this.clients = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n      this.$store.commit('setIsLoading', false);\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","clients","mounted","getClients","methods","$store","commit","get","then","response","catch","error","console","log"],"sources":["D:\\Courses\\web-programming-course\\homeworks\\6-crm\\crm_vue\\src\\views\\dashboard\\Clients.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"columns is-multiline\">\r\n      <div class=\"column is-12\">\r\n        <h1 class=\"title\">Clients</h1>\r\n\r\n        <router-link to=\"/dashboard/clients/add\">Add client</router-link>\r\n      </div>\r\n\r\n      <div class=\"column is-12\">\r\n        <template v-if=\"clients.length\">\r\n          <table class=\"table is-fullwidth\">\r\n            <thead>\r\n              <tr>\r\n                <th>Name</th>\r\n                <th>Contact person</th>\r\n                <th></th>\r\n              </tr>\r\n            </thead>\r\n\r\n            <tbody>\r\n              <tr v-for=\"client in clients\" :key=\"client.id\">\r\n                <td>{{ client.name }}</td>\r\n                <td>{{ client.contact_person }}</td>\r\n                <td>\r\n                  <router-link :to=\"{ name: 'Client', params: { id: client.id } }\">Details</router-link>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </template>\r\n        \r\n        <template v-else>\r\n          <p>You don't have any clients yet...</p>\r\n        </template>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'Clients',\r\n  data() {\r\n    return {\r\n      clients: []\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getClients()\r\n  },\r\n  methods: {\r\n    async getClients() {\r\n      this.$store.commit('setIsLoading', true)\r\n\r\n      await axios.get('/api/v1/clients/').then(response => {\r\n        // console.log(response.data);\r\n        this.clients = response.data\r\n      })\r\n        .catch(error => {\r\n          console.log(error);\r\n        })\r\n\r\n      this.$store.commit('setIsLoading', false)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped></style>"],"mappings":"AAyCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC;EAClB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,UAAUA,CAAA,EAAG;MACjB,IAAI,CAACE,MAAM,CAACC,MAAM,CAAC,cAAc,EAAE,IAAI;MAEvC,MAAMR,KAAK,CAACS,GAAG,CAAC,kBAAkB,CAAC,CAACC,IAAI,CAACC,QAAO,IAAK;QACnD;QACA,IAAI,CAACR,OAAM,GAAIQ,QAAQ,CAACT,IAAG;MAC7B,CAAC,EACEU,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC;MAEH,IAAI,CAACN,MAAM,CAACC,MAAM,CAAC,cAAc,EAAE,KAAK;IAC1C;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}