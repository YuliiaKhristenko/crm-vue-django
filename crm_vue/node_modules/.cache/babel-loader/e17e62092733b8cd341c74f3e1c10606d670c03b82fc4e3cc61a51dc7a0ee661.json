{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'Clients',\n  data() {\n    return {\n      clients: [],\n      showNextButton: false,\n      showPreviousButton: false,\n      currentPage: 1\n    };\n  },\n  mounted() {\n    this.getClients();\n  },\n  methods: {\n    goToNextPage() {\n      this.currentPage += 1;\n      this.getClients();\n    },\n    goToPreviousPage() {\n      this.currentPage -= 1;\n      this.getClients();\n    },\n    async getClients() {\n      this.$store.commit('setIsLoading', true);\n      this.showNextButton = false;\n      this.showPreviousButton = false;\n      await axios.get(`/api/v1/clients/?page=${this.currentPage}`).then(response => {\n        console.log('client response from server', response.data);\n        this.clients = response.data.results;\n        if (response.data.next) {\n          this.showNextButton = true;\n        }\n        if (response.data.previous) {\n          this.showPreviousButton = true;\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n      this.$store.commit('setIsLoading', false);\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","clients","showNextButton","showPreviousButton","currentPage","mounted","getClients","methods","goToNextPage","goToPreviousPage","$store","commit","get","then","response","console","log","results","next","previous","catch","error"],"sources":["D:\\Courses\\web-programming-course\\homeworks\\6-crm\\crm_vue\\src\\views\\dashboard\\Clients.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"columns is-multiline\">\r\n      <div class=\"column is-12\">\r\n        <h1 class=\"title\">Clients</h1>\r\n\r\n        <router-link to=\"/dashboard/clients/add\">Add client</router-link>\r\n      </div>\r\n\r\n      <div class=\"column is-12\">\r\n        <template v-if=\"clients.length\">\r\n          <table class=\"table is-fullwidth\">\r\n            <thead>\r\n              <tr>\r\n                <th>Name</th>\r\n                <th>Contact person</th>\r\n                <th></th>\r\n              </tr>\r\n            </thead>\r\n\r\n            <tbody>\r\n              <tr v-for=\"client in clients\" :key=\"client.id\">\r\n                <td>{{ client.name }}</td>\r\n                <td>{{ client.contact_person }}</td>\r\n                <td>\r\n                  <router-link :to=\"{ name: 'Client', params: { id: client.id } }\">Details</router-link>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n\r\n          <div class=\"buttons pagination is-centered\">\r\n            <button class=\"button pagination-previous is-light\" v-if=\"showPreviousButton\" @click=\"goToPreviousPage()\">Previous</button>\r\n            <button class=\" button pagination-next is-light\" v-if=\"showNextButton\" @click=\"goToNextPage()\">Next</button>\r\n          </div>\r\n        </template>\r\n\r\n        <template v-else>\r\n          <p>You don't have any clients yet...</p>\r\n        </template>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'Clients',\r\n  data() {\r\n    return {\r\n      clients: [],\r\n      showNextButton: false,\r\n      showPreviousButton: false,\r\n      currentPage: 1\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getClients()\r\n  },\r\n  methods: {\r\n    goToNextPage() {\r\n      this.currentPage += 1\r\n      this.getClients()\r\n    },\r\n    goToPreviousPage() {\r\n      this.currentPage -= 1\r\n      this.getClients()\r\n    },\r\n    async getClients() {\r\n      this.$store.commit('setIsLoading', true)\r\n\r\n      this.showNextButton = false\r\n      this.showPreviousButton = false\r\n\r\n      await axios.get(`/api/v1/clients/?page=${this.currentPage}`).then(response => {\r\n        console.log('client response from server',response.data);\r\n        this.clients = response.data.results\r\n\r\n        if (response.data.next) {\r\n          this.showNextButton = true\r\n        }\r\n        if (response.data.previous) {\r\n          this.showPreviousButton = true\r\n        }\r\n      })\r\n        .catch(error => {\r\n          console.log(error);\r\n        })\r\n\r\n      this.$store.commit('setIsLoading', false)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped></style>"],"mappings":"AA8CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,cAAc,EAAE,KAAK;MACrBC,kBAAkB,EAAE,KAAK;MACzBC,WAAW,EAAE;IACf;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC;EAClB,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACb,IAAI,CAACJ,WAAU,IAAK;MACpB,IAAI,CAACE,UAAU,CAAC;IAClB,CAAC;IACDG,gBAAgBA,CAAA,EAAG;MACjB,IAAI,CAACL,WAAU,IAAK;MACpB,IAAI,CAACE,UAAU,CAAC;IAClB,CAAC;IACD,MAAMA,UAAUA,CAAA,EAAG;MACjB,IAAI,CAACI,MAAM,CAACC,MAAM,CAAC,cAAc,EAAE,IAAI;MAEvC,IAAI,CAACT,cAAa,GAAI,KAAI;MAC1B,IAAI,CAACC,kBAAiB,GAAI,KAAI;MAE9B,MAAML,KAAK,CAACc,GAAG,CAAE,yBAAwB,IAAI,CAACR,WAAY,EAAC,CAAC,CAACS,IAAI,CAACC,QAAO,IAAK;QAC5EC,OAAO,CAACC,GAAG,CAAC,6BAA6B,EAACF,QAAQ,CAACd,IAAI,CAAC;QACxD,IAAI,CAACC,OAAM,GAAIa,QAAQ,CAACd,IAAI,CAACiB,OAAM;QAEnC,IAAIH,QAAQ,CAACd,IAAI,CAACkB,IAAI,EAAE;UACtB,IAAI,CAAChB,cAAa,GAAI,IAAG;QAC3B;QACA,IAAIY,QAAQ,CAACd,IAAI,CAACmB,QAAQ,EAAE;UAC1B,IAAI,CAAChB,kBAAiB,GAAI,IAAG;QAC/B;MACF,CAAC,EACEiB,KAAK,CAACC,KAAI,IAAK;QACdN,OAAO,CAACC,GAAG,CAACK,KAAK,CAAC;MACpB,CAAC;MAEH,IAAI,CAACX,MAAM,CAACC,MAAM,CAAC,cAAc,EAAE,KAAK;IAC1C;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}