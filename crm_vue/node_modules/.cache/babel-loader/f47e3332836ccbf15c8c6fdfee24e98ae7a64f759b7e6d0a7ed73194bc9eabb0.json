e => {\r\n        this.notes = response.data\r\n      })\r\n      .catch(error => {\r\n        console.log(error);\r\n      })\r\n\r\n      this.$store.commit('setIsLoading', false)\r\n    },\r\n    async deleteClient() {\r\n      this.$store.commit('setIsLoading', true)\r\n\r\n      const clientID = this.$route.params.id\r\n\r\n      await axios.post(`/api/v1/clients/delete_client/${clientID}/`).then(response => {\r\n        console.log('Deleted client', response.data);\r\n      })\r\n        .catch(error => {\r\n          console.log(error);\r\n        })\r\n      this.$router.push('/dashboard/leads')\r\n\r\n      this.$store.commit('setIsLoading', false)\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped></style>"],"mappings":";AA6CA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,CAAC,CAAC;MACVC,KAAK,EAAE;IACT;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,SAAS,CAAC;EACjB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,SAASA,CAAA,EAAG;MAChB,IAAI,CAACE,MAAM,CAACC,MAAM,CAAC,cAAc,EAAE,IAAI;MAEvC,MAAMC,QAAO,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAC;MAErC,MAAMb,KAAK,CAACc,GAAG,CAAE,mBAAkBJ,QAAS,GAAE,CAAC,CAACK,IAAI,CAACC,QAAO,IAAK;QAC/D,IAAI,CAACb,MAAK,GAAIa,QAAQ,CAACd,IAAG;MAC5B,CAAC,EACEe,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC;MAEH,MAAMlB,KAAK,CAACc,GAAG,CAAE,4BAA2BJ,QAAS,EAAC,CAAC,CAACK,IAAI,CAACC,QAAO,IAAK;QACvE,IAAI,CAACZ,KAAI,GAAIY,QAAQ,CAACd,IAAG;MAC3B,CAAC,EACAe,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC;MAED,IAAI,CAACV,MAAM,CAACC,MAAM,CAAC,cAAc,EAAE,KAAK;IAC1C,CAAC;IACD,MAAMY,YAAYA,CAAA,EAAG;MACnB,IAAI,CAACb,MAAM,CAACC,MAAM,CAAC,cAAc,EAAE,IAAI;MAEvC,MAAMC,QAAO,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAC;MAErC,MAAMb,KAAK,CAACsB,IAAI,CAAE,iCAAgCZ,QAAS,GAAE,CAAC,CAACK,IAAI,CAACC,QAAO,IAAK;QAC9EG,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAEJ,QAAQ,CAACd,IAAI,CAAC;MAC9C,CAAC,EACEe,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC;MACH,IAAI,CAA