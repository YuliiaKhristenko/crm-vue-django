lass=\"column is-12\">\r\n        <div class=\"buttons\">\r\n          <router-link :to=\"{ name: 'EditLead', params: { id: lead.id } }\" class=\"button is-light\">Edit</router-link>\r\n          <button class= \"button is-info\" @click=\"convertToClient\">Convert to client</button>\r\n          <button class= \"button is-danger\" @click=\"deleteLead\">Delete</button>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"column is-6\">\r\n        <div class=\"box\">\r\n          <h2 class=\"subtitle\">Details</h2>\r\n\r\n          <template v-if=\"lead.assigned_to\">\r\n            <p><strong>Assigned to:</strong> {{ lead.assigned_to.first_name }} {{ lead.assigned_to.last_name }}</p></template>\r\n          <p><strong>Status:</strong> {{ lead.status }}</p>\r\n          <p><strong>Priority:</strong> {{ lead.priority }}</p>\r\n          <p><strong>Confidence:</strong> {{ lead.confidence }}</p>\r\n          <p><strong>Estimated value:</strong> {{ lead.estimated }}</p>\r\n          <p><strong>Created at:</strong> {{ lead.created_at }}</p>\r\n          <p><strong>Modified at:</strong> {{ lead.modified_at }}</p>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"column is-6\">\r\n          <div class=\"box\">\r\n            <h2 class=\"subtitle\">Contact information</h2>\r\n\r\n            <p><strong>Contact person:</strong> {{ lead.contact_person }}</p>\r\n            <p><strong>Email:</strong> {{ lead.email }}</p>\r\n            <p><strong>Phone:</strong> {{ lead.phone }}</p>\r\n            <p><strong>Website:</strong> {{ lead.website }}</p>\r\n          </div>\r\n        </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nexport default {\r\n  name: 'Lead',\r\n  data() {\r\n    return {\r\n      lead: {}\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getLead()\r\n  },\r\n  methods: {\r\n    async getLead() {\r\n      this.$store.commit('setIsLoading', true)\r\n\r\n      const leadID = this.$route.params.id\r\n\